{
  "name": "Markdownizr",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Grab page content as markdown.",
  "homepage_url": "http://markdownizr.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "options_page": "src/options_custom/index.html",
  "browser_action": {
   "default_icon": "icons/icon19.png",
   "default_popup": "src/popup.html"
  },
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "contextMenus",
    "http://*/*"
  ]
}